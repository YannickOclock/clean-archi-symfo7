{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="h3 mb-3 font-weight-normal">Register</h1>

    {%  block errors %}
        {% for violation in viewModel.violations %}
            {% for path, message in violation %}
                {{ path }} : {{ message }} <br />
            {% endfor %}
        {% endfor %}
    {% endblock %}

    {{ form(form) }}
{% endblock %}

